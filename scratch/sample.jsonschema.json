{
	"$schema": "http://iglucentral.com/schemas/com.snowplowanalytics.self-desc/schema/jsonschema/1-0-0#",
	"description": "Schema for the focus of a form field",
	"self": {
		"vendor": "com.jake",
		"name": "action",
		"format": "jsonschema",
		"version": "1-0-0"
	},
	"type": "object",
	"properties": {
		"actionId": {
			"description": "The id of the performed action",
			"type": "string",
			"minLength": 0,
			"maxLength": 4096
		},
        "userId": {
            "description": "The identifier of the user who did the action",
            "type": "integer"
        }
	},
	"required": ["actionId", "userId"],
	"additionalProperties": false
}