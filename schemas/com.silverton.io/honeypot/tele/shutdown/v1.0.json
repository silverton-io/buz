{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "http://schemas.silverton.io/com.silverton.io/honeypot/tele/shutdown/v1.0.json",
    "title": "com.silverton.io/honeypot/tele/shutdown/v1.0.json",
    "description": "Honeypot shutdown event",
	"self": {
		"vendor": "com.silverton.io",
        "primaryNamespace": "honeypot",
        "secondaryNamespace": "tele",
		"name": "shutdown",
		"format": "jsonschema",
		"version": "1.0"
	},
    "type": "object",
    "properties": {
        "meta": {
            "type": "object",
            "description": "Metadata about the instance being shut down"
        },
        "time": {
            "type": "string",
            "format": "date-time",
            "description": "TS of the shutdown"
        },
        "elapsedSeconds": {
            "type": "number",
            "description": "The number of elapsed seconds since startup"
        }
    }
}