{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://schemas.silverton.io/com.silverton.io/honeypot/tele/beat/v1.0.json",
    "title": "com.silverton.io/honeypot/tele/beat/v1.0.json",
    "description": "Honeypot heartbeat event",
	"self": {
		"vendor": "com.silverton.io",
		"name": "honeypot/tele/beat",
		"format": "jsonschema",
		"version": "1.0"
	},
    "type": "object",
    "properties": {
        "meta": {
            "type": "object",
            "description": "Metadata about a beat"
        },
        "time": {
            "type": "string",
            "format": "date-time",
            "description": "TS of the beat"
        },
        "elapsedSeconds": {
            "type": "number",
            "description": "The number of elapsed seconds since startup"
        }
    }
}